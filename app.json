{
  "expo": {
    "name": "Contador Dutch",
    "slug": "dutch-contador",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon500.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.idaponte.dutchcontador",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon144.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.idaponte.dutchcontador"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon500.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "304ac36b-08f8-4760-b2b1-96180ea8c7ee"
      }
    }
  }
}
